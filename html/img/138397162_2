<script>
function redir(){
	var tourl = "http://blog.udn.com/";
	var locsearch = location.search;
	var loc = location.href;
	if(loc.indexOf("http://city.udn.com/blog?") > -1) {
		tourl = tourl + locsearch.substring(1, locsearch.length);
		window.location = tourl;		
	} else if ( loc.indexOf("http://city.udn.com/city?") > -1 ) {
		tourl = "http://city.udn.com/";
		tourl = tourl + locsearch.substring(1, locsearch.length);
		window.location = tourl;	
	} else if ( loc.indexOf("http://city.udn.com/allies?") > -1 ) {
		tourl = "http://blog.udn.com/blogs/";
		tourl = tourl + locsearch.substring(1, locsearch.length);
		window.location = tourl;			
	}
}
</script>
</head>
<body onLoad="javascript:redir();">
